    "#import <UIKit/UIKit.h>",
    "#import <Foundation/Foundation.h>",
    "#include <SlikeGlobals.h>",
    "#include <SlikeCoreShared.h>",
    "#include <SlikeReachability.h>",
    "#include <SlikeDeviceSettings.h>",
    "#include <SlikeUtilities.h>",
    "#include <SlikeNetworkManager.h>",
    "#include <SlikePlayer.h>",
    "#include <NSDictionary+Validation.h>"
  ],
  "frameworks": [
    "UIKit",
    "AdSupport",
    "SystemConfiguration"
  ],
  "dependencies": {
    "GoogleAds-IMA-iOS-SDK": [
      "~> 3.11.0"
    ],
    "FBAudienceNetwork": [

    ]
  },
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-all_load -lc++ -ObjC"
  }
