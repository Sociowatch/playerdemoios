{
  "name": "SlikePlayer",
  "version": "2.9.5",
  "summary": "SlikePlayer",
  "description": "Please refer to example demo and README.md for details of implementation.",
  "homepage": "http://timesinternet.in",
  "license": {
    "type": "Commercial",
    "file": "LICENSE"
  },
  "authors": {
    "Pravin Ranjan": "pravin.ranjan@timesinternet.in"
  },
  "source": {
    "git": "https://bitbucket.org/times_internet/slikeplayer-ios.git",
    "tag": "2.9.5"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": "SlikePlayer/Classes/**/*",
